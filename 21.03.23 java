class Main {
  public static void main(String[] args) {
    int v1 = 5;
    int v2 = 2;

    int result5 = v1 % v2;

    System.out.println(result5);


    int v3 = 4;
    int v4 = 2;

    int result6 = v3 % v4;
    
    System.out.println(result6);


    
    int v5 = 9;
    int v6 = 3;

    int result7 = v5 % v6;
    
    System.out.println(result7);


  }
}
